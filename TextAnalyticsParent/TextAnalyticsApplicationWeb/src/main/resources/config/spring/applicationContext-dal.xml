	</bean>

	<bean id="interactionSentimentIsMixed" class="com.verint.textanalytics.model.documentSchema.TextSchemaField" scope="singleton" lazy-init="true">
		<property name="name" value="#{T(com.verint.textanalytics.common.constants.TAConstants.SchemaFieldNames).interactionSentimentIsMixed}"> </property>
