	<bean id="sourceTypeField" class="com.verint.textanalytics.model.documentSchema.TextSchemaField" scope="singleton" lazy-init="true">
		<property name="name" value="#{T(com.verint.textanalytics.common.constants.TAConstants.SchemaFieldNames).sourceType}"> </property>
		<property name="displayFieldName" value="Source Type"> </property>
		<property name="fieldDataType" value="Text"></property>
		<property name="documentHierarchyType" value="Interaction"></property>
	</bean>	
		
	<bean id="scoreField" class="com.verint.textanalytics.model.documentSchema.TextSchemaField" scope="singleton" lazy-init="true">
		<property name="name" value="#{T(com.verint.textanalytics.common.constants.TAConstants.SchemaFieldNames).relevancyScore}"> </property>
		<property name="displayFieldName" value="Interaction_RelevancyScore"> </property>
		<property name="fieldDataType" value="Int"></property>
		<property name="documentHierarchyType" value="Interaction"></property>
	</bean>
	
	<bean id="dateFieldInteraction" class="com.verint.textanalytics.model.documentSchema.TextSchemaField" scope="singleton" lazy-init="true">
